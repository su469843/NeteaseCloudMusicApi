{
  "version": <=2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node@4.0.0"
    }
  ],
  "functions": {
    "app.js": {
      "maxDuration": 30,
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/app.js"
    }
  ],
  "installCommand": "npm install"
}
